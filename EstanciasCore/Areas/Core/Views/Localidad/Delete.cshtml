@model DAL.Models.Localidad
@{
    ViewData["Title"] = "Borrar Localidad";
}
<form method="post" asp-action="Delet" asp-controller="Localidad" asp-area="Core" asp-antiforgery="true">
    @Html.AntiForgeryToken()
    <div class="box-body">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <input asp-for="@Model.Id" type="hidden" />
        <div class="form-group">
            <label asp-for="Descripcion" class="control-label"></label>
            <input asp-for="Descripcion" class="form-control" disabled/>
        </div>
        <div class="form-group">
            <label asp-for="Latitud" class="control-label"></label>
            <input asp-for="Latitud" class="form-control" disabled/>
        </div>
        <div class="form-group">
            <label asp-for="Longitud" class="control-label"></label>
            <input asp-for="Longitud" class="form-control" disabled/>
        </div>
        <div class="form-group">
            <label class="control-label">Provincia</label>
            <select asp-items="@ViewBag.Provincias" asp-for="IdProvincia" class="form-control" searchable="true" disabled></select>
        </div>
    </div>
    <div class="box-footer">
        <a class="btn btn-danger" data-dismiss="modal">Cancelar</a>
        <button type="submit" class="btn btn-info pull-right">Borrar</button>
    </div>
</form>

